# Jira Configuration
# Copy this file to .env and fill in your actual values

# Jira Admin Token (required for Tempo API access)
REACT_APP_JIRA_ADMIN_TOKEN=your_jira_admin_token_here

# Jira URL (optional, defaults to https://jira.alchemytech.in)
REACT_APP_JIRA_URL=https://jira.alchemytech.in

# Debug mode (optional, defaults to false)
REACT_APP_DEBUG_JIRA=false
